/* pages/default_page/default_page.wxss */

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.no_vip {
  width: 100%;
  height: 100%;
  background-color: #fff;
  padding-top: 308rpx;
}

.gif_box {
  display: flex;
  justify-items: center;
  margin-bottom: 30rpx;
}

.gif_box image {
  width: 236rpx;
  height: 236rpx;
  margin: 0 auto;
}

.no_vip .tip {
  width: 100%;
  font-size: 26rpx;
  display: block;
  line-height: 45rpx;
  color: #999;
  text-align: center;
}
